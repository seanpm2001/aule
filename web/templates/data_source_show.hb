<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd>{{data_source.name}}</dd>
  <dt>Added</dt>
  <dd>{{dateFormat data_source.added}}</dd>
  <dt>Filetype</dt>
  <dd>{{data_source.filetype}}</dd>
  <dt>User</dt>
  <dd><a href="/users/{{escape data_source.user.uri}}">{{data_source.user.uri}}</a></dd>
</dl>
