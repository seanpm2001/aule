<h2>Samples</h2>

<ul class="nav nav-pills">
  <li class="active"><a href="/varda-web/samples">My samples</a></li>
  <li><a href="/varda-web/samples">All samples</a></li>
  <li><a href="/varda-web/add_sample">Add sample</a></li>
</ul>

<table class="table table-hover">
  <thead><tr><th>Added</th><th>Name</th></tr></thead>
  <tbody>
    {{#samples}}
    <tr data-href="/varda-web/samples/{{#escape}}{{uri}}{{/escape}}">
      <td>{{added}}</td>
      <td>{{name}}</td>
    </tr>
    {{/samples}}
  </tbody>
</table>
